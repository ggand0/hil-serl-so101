{
  "policy": {
    "type": "sac",
    "device": "cuda",
    "temperature_init": 0.01,
    "actor_learner_config": {
      "policy_parameters_push_frequency": 2
    },
    "storage_device": "cuda",
    "input_features": {
      "observation.images.gripper_cam": {
        "type": "VISUAL",
        "shape": [3, 128, 128]
      },
      "observation.state": {
        "type": "STATE",
        "shape": [6]
      }
    },
    "output_features": {
      "action": {
        "type": "ACTION",
        "shape": [4]
      }
    }
  },
  "env": {
    "type": "gym_manipulator",
    "fps": 10,
    "robot": {
      "type": "so101_follower_end_effector",
      "port": "/dev/ttyACM0",
      "id": "ggando_so101_follower",
      "disable_torque_on_disconnect": true,
      "use_degrees": false,
      "urdf_path": "/home/gota/ggando/ml/so101-playground/models/so101_new_calib.urdf",
      "target_frame_name": "gripper_frame_link",
      "end_effector_bounds": {
        "min": [0.1657, -0.0345, -0.0026],
        "max": [0.185, 0.0051, 0.0096]
      },
      "end_effector_step_sizes": {
        "x": 0.02,
        "y": 0.02,
        "z": 0.02
      },
      "cameras": {
        "gripper_cam": {
          "type": "opencv",
          "index_or_path": "/dev/video0",
          "fps": 30,
          "width": 640,
          "height": 480
        }
      }
    },
    "teleop": {
      "type": "so101_leader",
      "port": "/dev/ttyACM1",
      "id": "ggando_so101_leader",
      "use_degrees": false
    },
    "wrapper": {
      "control_mode": "leader",
      "display_cameras": true,
      "use_gripper": true,
      "crop_params_dict": null,
      "resize_size": [128, 128],
      "control_time_s": 20.0,
      "reset_time_s": 5.0,
      "fixed_reset_joint_positions": null,
      "add_joint_velocity_to_observation": false,
      "add_current_to_observation": false,
      "add_ee_pose_to_observation": false
    }
  },
  "dataset": {
    "repo_id": "ggando/so101_pick_lift_cube"
  },
  "wandb": {
    "enable": true,
    "project": "so101-hilserl"
  }
}
